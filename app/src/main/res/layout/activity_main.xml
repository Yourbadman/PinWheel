<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">


    <com.rocf.pinwheel.PinWheelWidget
        android:id="@+id/custom_progress5"
        android:layout_width="60dip"
        android:layout_height="60dip"
        android:layout_gravity="center"
        app:progress_background_color="@color/holo_darker_gray"
        app:progress_color="@android:color/white"
        app:progress_stroke_width="1dp" />
    <!--app:progress_end_color="@android:color/white"-->
    <!--app:progress_background_color="@color/holo_darker_gray"-->
    <!--app:progress_text_color="@color/holo_green_light"-->
    <Button
        android:id="@+id/start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start" />


    <Button
        android:id="@+id/end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="end" />
</LinearLayout>
